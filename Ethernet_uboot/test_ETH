echo ===== U-Boot ethernet test start=====
setenv eth1addr 00:04:9f:05:cd:35
setenv ethaddr 00:04:9f:04:68:b3
setenv eth2addr 00:04:9f:05:cd:36
setenv serial_number 001
setenv ipaddr $1
setenv ip2addr $2
setenv serverip $3
setenv ethact eth1
ping $3
setenv ethact eth0
ping $3
echo ===== U-Boot ethernet test end =====
